{
    "name": "Korean Learner",
    "version": "1.0",
    "description": "Pop ups of korean vocab!",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'",
    "permissions": ["activeTab"]
}